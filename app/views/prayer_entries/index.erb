Prayer Index
<br><br>
<% @prayer_entries.each do |prayer_entry| %>
    <h4><a href="/prayer_entries/<%= prayer_entry.id %>"<%= prayer_entry.created_at %></a> </h4>
    <strong><%= prayer_entry.title %></strong>
    <p><%= prayer_entry.content %></p>
    <p>By <%= prayer_entry.user.name%></p>
    <br><br>
<% end %>